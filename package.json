{
  "name": "family-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --exit-child --watch src api/index.ts",
    "run-container": "docker run --name family_db -e MYSQL_ROOT_PASSWORD=1234 -p 3306:3306 -d mysql",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "lint": "eslint --ext .ts .",
    "build": "tsc",
    "format:check": "prettier --check 'api/**/*.{js,ts,json,yaml,yml,md}'",
    "format:write": "prettier --write 'api/**/*.{js,ts,json,yaml,yml,md}'",
    "preparePullRequest": "npm run test && npm run format:check && npm run lint"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "keywords": [],
  "author": "WAGMI Studios",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "await-to-js": "^3.0.0",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "toad-scheduler": "^1.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
