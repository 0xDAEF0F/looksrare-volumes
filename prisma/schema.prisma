generator client {
  provider = "prisma-client-js"
}

generator nexusPrisma {
  provider = "nexus-prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// This represents an exchange
model Exchange {
  id           Int    @id @default(autoincrement())
  name         String @unique
  ticker       String @unique
  tokenAddress String @unique
  tokenSupply  Int?
  tokenCap     Int?
}
